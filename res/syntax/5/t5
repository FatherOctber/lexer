NON-TERMINALS: [ EQUEXP, ADDEXPRES, EXPRESSION, LOGICEXP, MATROPER, MULTEXP, NUMBER, PRIMARY, TERNARYEXP, VAR]
TERMINALS:     [addit, bool, compare, cp, cto, height, id, int, logic, mult, op, qto, real, transpose, weight]
START SYMBOL:  EXPRESSION
     RULES:
(0)  EQUEXP -> ADDEXPRES compare ADDEXPRES
(1) ADDEXPRES -> MULTEXP addit MULTEXP
(2) MULTEXP -> PRIMARY mult PRIMARY
(3) PRIMARY -> op EXPRESSION cp
(4) PRIMARY -> NUMBER
(5) PRIMARY -> VAR
(6) NUMBER -> int
(7) NUMBER -> real
(8) NUMBER -> bool
(9) VAR -> id
(10) LOGICEXP ->  EQUEXP logic  EQUEXP
(11) PRIMARY -> MATROPER
(12) MATROPER -> height op VAR cp
(13) MATROPER -> weight op VAR cp
(14) MATROPER -> transpose op VAR cp
(15) EXPRESSION -> TERNARYEXP
(16) TERNARYEXP -> LOGICEXP qto LOGICEXP cto LOGICEXP
(17) TERNARYEXP -> LOGICEXP
(18) LOGICEXP ->  EQUEXP
(19)  EQUEXP -> ADDEXPRES
(20) ADDEXPRES -> MULTEXP
(21) MULTEXP -> PRIMARYГрамматика операторного предшествования
